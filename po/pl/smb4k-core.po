#. i18n: ectx: label, entry (EncryptionLevel), group (Network)
#: smb4k.kcfg:207
#, kde-format
msgid "None"
msgstr "Brak"

#. i18n: ectx: label, entry (EncryptionLevel), group (Network)
#: smb4k.kcfg:210
#, kde-format
msgid "Request"
